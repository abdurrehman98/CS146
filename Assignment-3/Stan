        "# Stan model inputs\r\n",
        "stan_data = {\r\n",
        "    \"alpha\": 1,\r\n",
        "    \"beta\": 0.25,\r\n",
        "    \"n\": len(waiting_times_hour),\r\n",
        "    \"waiting_times_hour\": waiting_times_hour,\r\n",
        "}\r\n",
        "\r\n",
        "results = call_center_stan.sampling(data=stan_data)\r\n",
